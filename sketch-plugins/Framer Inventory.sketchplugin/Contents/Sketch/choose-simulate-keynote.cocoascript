@import 'actions/actionFactory.cocoascript'

var runSimulateKeynoteIdentifier = 2
var currentCommandIdentifier = runSimulateKeynoteIdentifier
var showLog = true
var testing = false

var onRun = function(context) {
	if (showLog) { log("Running Simulate Keynote") }
	chooseAction(context, currentCommandIdentifier, testing)
}